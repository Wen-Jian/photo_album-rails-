<%= form_for @photo, :url => {:controller => "photos", :action => "update", :method => :patch} do |f| %>
	<%= f.label :title, "Title" %>
	<%= f.text_field :title %>
	<%= f.label :date, "Date" %>
	<%= f.text_field :date %>
	<%= f.label :description, "Description" %>
	<%= f.text_area :description %>
	<%= f.label :file_location, "file_location" %>
	<%= f.text_field :file_location %>

	<%= f.submit "Update" %>
<% end %>
